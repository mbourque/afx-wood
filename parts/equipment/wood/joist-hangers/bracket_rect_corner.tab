BEGIN_ASM_DESCR

SEARCH_MDL_PARAM THIS L L
SEARCH_MDL_PARAM THIS H H
SEARCH_MDL_PARAM THIS S S
SEARCH_MDL_PARAM_OPTIONAL THIS OFF_L OFF_L
SEARCH_MDL_PARAM_OPTIONAL THIS OFF_H OFF_H
SEARCH_MDL_PARAM_OPTIONAL THIS L1 L1
SEARCH_MDL_PARAM_OPTIONAL THIS WITH_CHAMFER_1 WITH_CHAMFER_1

IF_1 ELEM_NEW
  CONFIG_ELEM
  REGEN_MDL THIS
END_IF_1

IF_2 ELEM_EXIST
  CONFIG_ELEM_EXIST
END_IF_2		

SEARCH_MDL_REF THIS PLANE XZ PLATE_XZ		
SEARCH_MDL_REF THIS PLANE YZ PLATE_YZ		
SEARCH_MDL_REF THIS PLANE XY PLATE_XY
SEARCH_MDL_REF THIS PLANE MINUS_XY PLATE_MINUS_XY		
SEARCH_MDL_REF THIS PLANE CENTER_DTM PLATE_CENTER		
SEARCH_MDL_REF THIS POINT REF_PNT PLATE_POINT		


SEARCH_PROF_SYSTEM_BORE_ENDPNT PROF_END PROF_END_PNT
SEARCH_PROF_PERPEND_DATUM PROF_END PROF_PERPEND_DATUM
	
IF_4 REF_INVALID PROF_SIDE_FACE

  IF_41 NORM_VECS_RIGHT_HAND_CSYS PROF_END ATTACH_FACE PROF_PERPEND_DATUM
    ASSEMBLE THIS
    ALIGN PROF_END PLATE_XZ
    PNT_ON_SRF ATTACH_FACE PLATE_POINT
    PNT_ON_SRF PROF_END_PNT PLATE_CENTER
    ORIENT PROF_PERPEND_DATUM PLATE_MINUS_XY
    END_ASSEMBLE
  END_IF_41

  IF_42 NORM_VECS_LEFT_HAND_CSYS PROF_END ATTACH_FACE PROF_PERPEND_DATUM
    ASSEMBLE THIS
    ALIGN PROF_END PLATE_XZ
    PNT_ON_SRF ATTACH_FACE PLATE_POINT
    PNT_ON_SRF PROF_END_PNT PLATE_CENTER
    ORIENT PROF_PERPEND_DATUM PLATE_XY
    END_ASSEMBLE
  END_IF_42

END_IF_4

IF_5 REF_VALID PROF_SIDE_FACE

  IF_51 NORM_VECS_RIGHT_HAND_CSYS PROF_END ATTACH_FACE PROF_SIDE_FACE
    ASSEMBLE THIS
    ALIGN PROF_END PLATE_XZ
    PNT_ON_SRF ATTACH_FACE PLATE_POINT
    ALIGN PROF_SIDE_FACE PLATE_MINUS_XY
    END_ASSEMBLE
  END_IF_51

  IF_52 NORM_VECS_LEFT_HAND_CSYS PROF_END ATTACH_FACE PROF_SIDE_FACE
    ASSEMBLE THIS
    ALIGN PROF_END PLATE_XZ
    PNT_ON_SRF ATTACH_FACE PLATE_POINT
    ALIGN PROF_SIDE_FACE PLATE_XY
    END_ASSEMBLE
  END_IF_52

END_IF_5			



END_ASM_DESCR			


