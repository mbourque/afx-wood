BEGIN_ASM_DESCR			

SEARCH_MDL_PARAM THIS L1 L1			
SEARCH_MDL_PARAM THIS L2 L2			
SEARCH_MDL_PARAM THIS H H			
SEARCH_MDL_PARAM THIS S S			

IF_1 ELEM_NEW			
CONFIG_ELEM			
END_IF_1			

IF_2 ELEM_EXIST			
CONFIG_ELEM_EXIST
END_IF_2


SEARCH_MDL_REF PROF_SURFACE PLANE XY PROF_XY

SEARCH_MDL_REF THIS PLANE XZ LASCHE_XZ
SEARCH_MDL_REF THIS PLANE YZ LASCHE_YZ
SEARCH_MDL_REF THIS PLANE XY LASCHE_XY
SEARCH_MDL_REF THIS POINT REF_PNT LASCHE_POINT

IF_4 REF_INVALID PARALLEL_PLANE
ASSEMBLE THIS
ALIGN PROF_SURFACE LASCHE_XZ
PNT_ON_SRF PLACEMENT_POINT LASCHE_YZ
PNT_ON_SRF PLACEMENT_POINT LASCHE_XY
ORIENT PROF_XY LASCHE_YZ
END_ASSEMBLE
END_IF_4

IF_5 REF_VALID PARALLEL_PLANE
ASSEMBLE THIS
PNT_ON_SRF PROF_SURFACE LASCHE_POINT
PNT_ON_SRF PLACEMENT_POINT LASCHE_YZ
PNT_ON_SRF PLACEMENT_POINT LASCHE_XY
ORIENT PROF_XY LASCHE_YZ
ORIENT PARALLEL_PLANE LASCHE_XY
END_ASSEMBLE
END_IF_5

END_ASM_DESCR


