BEGIN_ASM_DESCR		

SEARCH_MDL_PARAM THIS L1 L1		
SEARCH_MDL_PARAM THIS L2 L2		
SEARCH_MDL_PARAM THIS H H		
SEARCH_MDL_PARAM THIS S S		
SEARCH_MDL_PARAM THIS CUT_OFFSET CUT_OFFSET		
SEARCH_MDL_PARAM THIS MIDDLE_ASSEMBLY MIDDLE_ASSEMBLY		
SEARCH_MDL_PARAM THIS WITH_CUT WITH_CUT		

IF_1 ELEM_NEW		
CONFIG_ELEM		
END_IF_1		

IF_2 ELEM_EXIST		
CONFIG_ELEM_EXIST
END_IF_2

SEARCH_PROF_SYSTEM_BORE_ENDPNT PROF_SIDE_FACE PROF_END_PNT
SEARCH_MDL_REF PROF_SIDE_FACE PLANE XY PROF_XY

SEARCH_MDL_REF THIS PLANE XZ LASCHE_XZ
SEARCH_MDL_REF THIS PLANE YZ LASCHE_YZ
SEARCH_MDL_REF THIS PLANE XY LASCHE_XY
SEARCH_MDL_REF THIS POINT REF_PNT LASCHE_POINT
SEARCH_MDL_REF THIS PLANE REV_YZ LASCHE_REV_YZ
IF_21 PARAM_TRUE WITH_CUT
SEARCH_MDL_REF THIS QUILT PROFIL_CUT_QUILT LASCHE_CUT_QUILT
END_IF_21

IF_3 PARAM_FALSE MIDDLE_ASSEMBLY
IF_31 SEL_PROF_SIDE PROF_SIDE_FACE START_SIDE
ASSEMBLE THIS
ALIGN PROF_SIDE_FACE LASCHE_XY
PNT_ON_SRF PROF_END_PNT LASCHE_YZ
PNT_ON_SRF PROF_END_PNT LASCHE_XZ
ORIENT PROF_XY LASCHE_YZ
END_ASSEMBLE
END_IF_31
IF_32 SEL_PROF_SIDE PROF_SIDE_FACE END_SIDE
ASSEMBLE THIS
ALIGN PROF_SIDE_FACE LASCHE_XY
PNT_ON_SRF PROF_END_PNT LASCHE_YZ
PNT_ON_SRF PROF_END_PNT LASCHE_XZ
ORIENT PROF_XY LASCHE_REV_YZ
END_ASSEMBLE
END_IF_32
END_IF_3

IF_4 PARAM_TRUE MIDDLE_ASSEMBLY
IF_41 SEL_PROF_SIDE PROF_SIDE_FACE START_SIDE
ASSEMBLE THIS
ALIGN PROF_END_PNT LASCHE_POINT
ORIENT PROF_SIDE_FACE LASCHE_XY
ORIENT PROF_XY LASCHE_YZ
END_ASSEMBLE
END_IF_41
IF_42 SEL_PROF_SIDE PROF_SIDE_FACE END_SIDE
ASSEMBLE THIS
ALIGN PROF_END_PNT LASCHE_POINT
ORIENT PROF_SIDE_FACE LASCHE_XY
ORIENT PROF_XY LASCHE_REV_YZ
END_ASSEMBLE
END_IF_42
END_IF_4

IF_5 PARAM_TRUE WITH_CUT			
CREATE_UDF QUILT_CUT PROF_SIDE_FACE			
UDF_REF REF_QUILT LASCHE_CUT_QUILT			
END_CREATE_UDF			
END_IF_5			

END_ASM_DESCR			



